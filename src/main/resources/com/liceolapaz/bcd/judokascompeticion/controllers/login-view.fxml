<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>
    <VBox alignment="CENTER" maxHeight="500" maxWidth="300" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.liceolapaz.bcd.judokascompeticion.controllers.LogInController">
        <children>
            <Label contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" text="Inicia Sesión" textAlignment="CENTER">
                <font>
                    <Font name="DejaVu Sans Bold" size="25.0" />
                </font>
                <VBox.margin>
                    <Insets />
                </VBox.margin>
            </Label>
            <Label text="Nombre de usuarío:">
                <font>
                    <Font name="System Bold" size="18.0" />
                </font>
                <opaqueInsets>
                    <Insets />
                </opaqueInsets>
                <VBox.margin>
                    <Insets />
                </VBox.margin>
            </Label>
            <TextField fx:id="usuario" maxWidth="300.0" minWidth="300.0" promptText="Usuario" />
            <Label text="Contraseña:">
                <font>
                    <Font name="System Bold" size="18.0" />
                </font>
            </Label>
            <PasswordField fx:id="contrasena" maxWidth="300.0" minWidth="300.0" promptText="Contraseña" />
            <Button fx:id="boton" text="_Iniciar sesion" onAction="#onClic" />
        </children>
    </VBox>

